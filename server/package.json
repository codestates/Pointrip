{
  "name": "pointrip-server",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node ./build/app.js",
    "test" : "nodemon src/app.ts",
    "dev": "ts-node-dev src/app.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n Test",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "build": "tsc --build",
    "clean": "tsc --build --clean"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.6",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@mochajs/json-file-reporter": "^1.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.36",
    "aws-sdk": "^2.1013.0",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "nodemon": "^2.0.16",
    "supertest": "^4.0.2",
    "ts-node": "^10.8.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
